dialecte:fran√ßais;

structure Employee { 
  ID: entier, 
  Salary: entier, 
}

fonction count_high_earners(employees: [5]Employee, threshold: entier) entier {
  variable high_earner_count: entier = 0;
  variable idx: entier = 0;
  variable array_size: entier = 5;
  
  tantque (idx < array_size) {
    variable current_salary: entier = employees[idx].Salary;
    
    si (current_salary > threshold) {
      high_earner_count = high_earner_count + 1;
    }
    
    idx = idx + 1;
  }
  
  retourner high_earner_count;
}

demarrer() entier {
  variable THRESHOLD_CONST: entier = 50000;
  
  variable employees_array: [5]Employee = [5]Employee{
    Employee{ID: 105, Salary: 30000}, // Below
    Employee{ID: 101, Salary: 45000}, // Below
    Employee{ID: 102, Salary: 60000}, // Above
    Employee{ID: 103, Salary: 50000}, // Equal, not >
    Employee{ID: 104, Salary: 75000}, // Above
  };
  
  variable count: entier = count_high_earners(employees_array, THRESHOLD_CONST);
  
  afficher("Employees with salary above %d: %d\n", THRESHOLD_CONST, count);
  
  retourner 0;
}
