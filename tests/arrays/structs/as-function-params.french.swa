dialecte:franÃ§ais;

structure Point { 
  X: entier, 
  Y: entier, 
}

fonction count_x_occurrences(points: [4]Point, target_x: entier) entier {
  variable count: entier = 0;
  variable idx: entier = 0;
  variable array_size: entier = 4;
  
  tantque (idx < array_size) {
    si (points[idx].X == target_x) {
      count = count + 1;
    }
    
    idx = idx + 1;
  }
  
  retourner count;
}

demarrer() entier {
  variable points_array: [4]Point = [4]Point{
    Point{X: 1, Y: 10}, 
    Point{X: 5, Y: 20},
    Point{X: 1, Y: 30}, // Match
    Point{X: 5, Y: 40}, 
  };
  
  variable target: entier = 1;
  variable occurrences: entier = count_x_occurrences(points_array, target);
  afficher("X=%d occurs %d times, ", target, occurrences);

  target = 5;
  occurrences = count_x_occurrences(points_array, target);
  afficher("X=%d occurs %d times, ", target, occurrences);

  target = 99;
  occurrences = count_x_occurrences(points_array, target);
  afficher("X=%d occurs %d times", target, occurrences);
  
  retourner 0;
}
