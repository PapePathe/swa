dialect:english;

func swap(list: [10]int, i: int, j: int) int {
  let temp: int = list[i];
  list[i] = list[j];
  list[j] = temp;
  return 0;
}

func bubble_sort(list: [10]int) int {
  let N: int = 10;
  let i: int = 0;
  let j: int = 0;
  
  while (i < (N - 1)) {
    j = 0;
    while (j < (N - 1 - i)) {
      if (list[j] > list[j + 1]) {
        let temp: int = list[j];
        list[j] = list[j + 1];
        list[j + 1] = temp;
      }
      j = j + 1;
    }
    i = i + 1;
  }
  return 0;
}

func print_array(list: [10]int) int {
  let N: int = 10;
  let i: int = 0;
  print("Sorted Array: ");
  while (i < N) {
    print("%d ", list[i]);
    i = i + 1;
  }
  print("\n");
  return 0;
}

start() int {
  let numbers: [10]int = [10]int{64, 34, 25, 12, 22, 11, 90, 8, 77, 4};
  
  bubble_sort(numbers);
  print_array(numbers);
  
  return 0;
}
