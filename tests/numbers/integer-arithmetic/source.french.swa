dialecte:franÃ§ais;

demarrer() entier {
  variable a: entier = 10;
  variable b: entier = 5;
  variable sum: entier = a + b;

  si(sum == 15) {
    afficher("%s", "ok");
  } sinon {
    afficher("%s", "Integer addition failed");
  }

  si(10 + 5 == 15) {
    afficher("%s", "ok");
  } sinon {
    afficher("%s", "Integer literal addition failed");
  }

  si(a > b) {
    afficher("%s", "ok");
  } sinon {
    afficher("%s", "Integer greater than failed");
  }

  si(a >= b) {
    afficher("%s", "ok");
  } sinon {
    afficher("%s", "Integer greater than or equal failed");
  }

  si(b < a) {
    afficher("%s", "ok");
  } sinon {
    afficher("%s", "Integer less than failed");
  }

  si(b <= a) {
    afficher("%s", "ok");
  } sinon {
    afficher("%s", "Integer less than or equal failed");
  }

  retourner 0;
}
