dialect:french;

demarrer() entier {
  variable salaire_de_lucien: entier = 500000;
  variable salaire_de_pathe : entier = 50000;

  si(salaire_de_lucien >= salaire_de_pathe) {
    afficher("%s", "ok");
  } sinon {
    afficher("%s", "Ast BinaryExpression GreaterThanEqualsImplementationError");
  }

  si(salaire_de_pathe >= salaire_de_lucien) {
    afficher("%s", "Ast BinaryExpression GreaterThanEqualsImplementationError cond: (salaire_de_pathe >= salaire_de_lucien)");
  } sinon {
    afficher("%s", "ok");
  }

  retourner 0;
}
