dialect:english;

func inner() (int, int) {
  return 1, 2;
}

func outer() (int, int) {
  let a: int;
  let b: int;
  (a, b) = inner();
  return a + b, a - b;
}

// Test 10: Nested tuple returns â€“ inner call used in outer return
start() int {
  let x: int;
  let y: int;
  (x, y) = outer();
  // Expected: 3, -1
  print("x=%d, y=%d", x, y);
  return 0;
}


