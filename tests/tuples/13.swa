func nextPair(n: int) (int, int) {
  return n, n*2;
}
start() int {
  let i: int = 0;
  while (i < 3) {
    let a: int;
    let b: int;
    (a, b) = nextPair(i);
    print("i=%d: (%d,%d)", i, a, b);
    i = i + 1;
  }
  return 0;
}
