// Test 15: Recursive function returning multiple (Fibonacci pair)
func fibPair(n: int) (int, int) {
  if (n == 0) { return 0, 1; }
  let a: int;
  let b: int;
  (a, b) = fibPair(n -1);
  return b, a + b;
}
start() int {
  let prev: int;
  let curr: int;
  (prev, curr) = fibPair(5);
  print("fib(5)=%d, fib(6)=%d", prev, curr); // Expected: 5, 8
  return 0;
}
