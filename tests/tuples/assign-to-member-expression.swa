dialect:english;

func divmod(a: int, b: int) (int, int, error) {
    if (b == 0) {
        return 0, 0, error("dividend is zero"); 
    }

    let div : int = a / b;
    let mod: int = a % b;

    return div, mod, zero(error); 
}

struct Result {
  div: int,
  mod: int,
}

start() int {
  let p : Result;
  let err: error;
  let div : int;
  let mod: int;
   (p.div, p.mod, err) = divmod(100, 23);
 // (div, mod, err) = divmod(100, 25);

  if(err != zero(error)) {
    print("Division by zero error (%s)", err);
    return -1;
  }

  // print("div = %d, mod = %d", div, mod);
   print("p.div = %d, p.mod = %d", p.div, p.mod);

  return 0;
}
