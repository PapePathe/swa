dialect:english;

start() int {
    let x: int = 10;
    print("Outer x: %d\n", x);
    
    {
        let x: int = 20;
        print("Inner x: %d\n", x);
        if (x != 20) {
            print("Error: Shadowing failed in inner block.\n");
            return 1;
        }
    }
    
    print("Outer x again: %d\n", x);
    if (x != 10) {
        print("Error: Outer variable was corrupted by inner shadow.\n");
        return 1;
    }
    
    print("Success: Variable shadowing works.\n");
    return 0;
}
