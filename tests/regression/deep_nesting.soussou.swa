khuien:soussou;

fokhi Level1 {
    val: konti,
}

fokhi Level2 {
    l1: Level1,
}

fokhi Level3 {
    l2: Level2,
}

fokhi Level4 {
    l3: Level3,
}

sodé() konti {
    kouicé deep: Level4 = Level4{
        l3: Level3{
            l2: Level2{
                l1: Level1{
                    val: 99
                }
            }
        }
    };
    
    masen("Deep value: %d\n", deep.l3.l2.l1.val);
    
    xa (deep.l3.l2.l1.val == 99) {
        masen("Success: Deep struct nesting works.\n");
    } xamuara {
        masen("Error: Deep struct nesting failed.\n");
        gbilen 1;
    }
    
    gbilen 0;
}
