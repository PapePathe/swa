dialecte:fran√ßais;

fonction add(first: decimal, last: decimal) decimal {
  retourner first + last;
}

structure Float{
  v: decimal,
}

demarrer () entier {
  variable p : [1]Float= [1]Float{
    Float{v: 10.25},
  };
  variable res : decimal = add(p[0], 4.75);
  afficher("10.25 + 4.75 = %.2f", res);
  retourner 0; 
}
