dialect:english;

struct Person {
  Age:  int,
  Name: string, 
}

func changeAge(person: Person) int {
  person.Age = 40;

  return person.Age;
}

start() int {
  let p : Person = Person{ Age: 0, Name: "Pathe", };
  let myAge : int = 0;
  myAge = changeAge(p);
 
  print("age: %d, name: %s", myAge, p.Name);

  return 0; 
}
