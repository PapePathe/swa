dialect:english;

struct Person {
  Age:     int,
  Height:  float,
  Name:    string,
}

func changeAge(person: Person) int {
  person.Age = 40;

  return person.Age;
}

func changHeight(person: Person) float {
  person.Height= 1.80;

  return person.Height;
}

start() int {
  let p : Person = Person{ Age: 0, Height: 0.0, Name: "Pathe", };
  let myAge : int = changeAge(p);
  let myHeight : float = changHeight(p);
 
  print("age: %d, height: %.2f, name: %s", myAge, myHeight, p.Name);

  return 0; 
}
