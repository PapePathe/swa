dialect:english;

struct Person {
  Age:     int,
  Height:  float,
  Name:    string,
}

func changeAge(person: Person) int {
  person.Age = person.Age + 20;
  person.Age = 20 + person.Age;
 

  return person.Age;
}

func changeName(person: Person) string {
  person.Name = "Pathe SENE";

  return person.Name;
}

func changHeight(person: Person) float {
  person.Height= 1.80;

  return person.Height;
}

start() int {
  let p : Person = Person{ Age: 0, Height: 0.0, Name: "Pathe", };
  let myAge : int = changeAge(p);
  let myHeight : float = changHeight(p);
  let myName: string = changeName(p);
 
  print("age: %d, height: %.2f, name: %s", myAge, myHeight, myName);

  return 0; 
}
