dialecte:fran√ßais;

structure Point {
  x : decimal,
}

fonction add(first: decimal, last: decimal) decimal {
  retourner first + last;
}

demarrer () entier {
  variable p : Point = Point{x: 10.25};
  variable res : decimal = add(p.x, 4.75);
  afficher("10.25 + 4.75 = %.2f", res);
  retourner 0; 
}
