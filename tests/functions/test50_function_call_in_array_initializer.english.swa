dialect:english;

// Test 50: Function call in array initializer
func gen(i: int) int {
  return i * i;
}
start() int {
  let arr: [5]int = [5]int{gen(0), gen(1), gen(2), gen(3), gen(4)};
  let i: int = 0;
  while (i < 5) {
    print("arr[%d] = %d\n", i, arr[i]);
    i = i + 1;
  }
  // Expected: 0,1,4,9,16
  return 0;
}
