dialecte:fran√ßais;

structure MultiPointer {
  ptr1: *entier,
  ptr2: *entier,
  value: entier,
}

demarrer() entier {
  variable m : MultiPointer = MultiPointer{
    ptr1: [2]entier{10, 20},
    ptr2: [3]entier{100, 200, 300},
    value: 99,
  };

  afficher("ptr1[0]=%d ptr1[1]=%d ptr2[0]=%d ptr2[2]=%d value=%d", 
        m.ptr1[0], m.ptr1[1], m.ptr2[0], m.ptr2[2], m.value);
  retourner 0;
}
