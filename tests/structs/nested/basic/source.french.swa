dialecte:fran√ßais;

structure Address {
  City: chaine,
  ZipCode: entier,
}

structure Person {
  Name: chaine,
  Age: entier,
  HomeAddress: Address,
}

demarrer() entier {
  variable addr: Address = Address{
    City: "Dakar",
    ZipCode: 12000,
  };

  variable person: Person = Person{
    Name: "Pathe",
    Age: 30,
    HomeAddress: addr,
  };
  
  afficher(
    "Name: %s, City: %s, ZipCode: %d", 
    person.Name, person.HomeAddress.City, person.HomeAddress.ZipCode
  );
  
  retourner 0;
}
