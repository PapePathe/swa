khuien:soussou;

fokhi Address {
  City: sèbèli,
  ZipCode: konti,
}

fokhi Person {
  Name: sèbèli,
  Age: konti,
  HomeAddress: Address,
}

sodé() konti {
  kouicé addr: Address = Address{
    City: "Dakar",
    ZipCode: 12000,
  };
  
  masen("City: %s,  ZipCode: %d", addr.City, addr.ZipCode);
  
  kouicé person: Person = Person{
    Name: "Pathe",
    Age: 30,
    HomeAddress: addr,
  };
  
  masen("Name: %s", person.Name);
  masen("City from person: %s", person.HomeAddress.City);
  
  gbilen 0;
}
