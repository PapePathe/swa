dialect:english;

struct Address {
  City: string,
  ZipCode: int,
}

struct Person {
  Name: string,
  Age: int,
  HomeAddress: Address,
}

start() int {
  let person: Person = Person{
    Name: "Pathe",
    Age: 30,
    HomeAddress: Address{
      City: "Dakar",
      ZipCode: 12000,
    },
  };
  
  print("Name: %s, City: %s, ZipCode: %d", person.Name, person.HomeAddress.City, person.HomeAddress.ZipCode);
  
  return 0;
}
