dialect:french;

struct Adresse {
  Ville: string,
  CodePostal: int,
}

struct Personne {
  Nom: string,
  Age: int,
  AdresseDomicile: Adresse,
}

debut() int {
  soit personne: Personne = Personne{
    Nom: "Sene",
    Age: 25,
    AdresseDomicile: Adresse{
      Ville: "Thies",
      CodePostal: 21000,
    },
  };
  
  soit ville: string = personne.AdresseDomicile.Ville;
  soit codePostal: int = personne.AdresseDomicile.CodePostal;
  
  affiche("Nom: %s, Ville: %s, CodePostal: %d", personne.Nom, ville, codePostal);
  
  retour 0;
}
