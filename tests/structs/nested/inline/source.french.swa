dialecte:fran√ßais;

structure Address {
  City: chaine,
  ZipCode: entier,
}

structure Person {
  Name: chaine,
  Age: entier,
  HomeAddress: Address,
}

demarrer() entier {
  variable person: Person = Person{
    Name: "Sene",
    Age: 25,
    HomeAddress: Address{
      City: "Thies",
      ZipCode: 21000,
    },
  };
  
  variable city: chaine = person.HomeAddress.City;
  variable zipcode: entier = person.HomeAddress.ZipCode;
  
  afficher("Name: %s, City: %s, ZipCode: %d", person.Name, city, zipcode);
  
  retourner 0;
}
