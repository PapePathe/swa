dialect:english;

// Test: Passing struct with embedded array to function
struct Data {
  arr: [3]int,
  value: int,
}

func processData(d: Data) int {
  return d.arr[0] + d.arr[1] + d.arr[2] + d.value;
}

start() int {
  let d : Data = Data{
    arr: [3]int{10, 20, 30},
    value: 5,
  };

  let result: int = processData(d);
  print("Result: %d", result);
  return 0;
}
