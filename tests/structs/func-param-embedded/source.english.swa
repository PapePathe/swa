dialect:english;

struct Data {
  arr: [3]int,
  value: int,
}

// TODO: struct should be passed as a value here
// as it's not changing a value here
func processData(d: *Data) int {
  return d.arr[0] + d.arr[1] + d.arr[2] + d.value;
}

start() int {
  let d : Data = Data{
    arr: [3]int{10, 20, 30},
    value: 5,
  };

  let result: int = processData(d);
  print("Result: %d", result);
  return 0;
}
