khuien:soussou;

fokhi Data {
  arr: [3]konti,
  value: konti,
}

// TODO: struct should be passed as a value here
// as it's not changing a value here
wali processData(d: *Data) konti {
  gbilen d.arr[0] + d.arr[1] + d.arr[2] + d.value;
}

sodé() konti {
  kouicé d : Data = Data{
    arr: [3]konti{10, 20, 30},
    value: 5,
  };

  kouicé result: konti = processData(d);
  masen("Result: %d", result);
  gbilen 0;
}
