dialect:english;

struct Dimensions {
  Length: float,
  Width:  float,
  Height: int,
  Name:   string,
}

struct Object {
  Dim:   Dimensions,
  Shape: string,
}

struct Block {
  top:    Object,
  bottom: Object,
}

start() int {
  let dim : Dimensions = Dimensions{};
  print("- Dimension with zero value\n");
  print(
    "Length: %.2f, Width: %d, Height: %d, Name: %s\n\n", 
    dim.Length, dim.Width, dim.Height, dim.Name,
  );

  let obj : Object = Object{};
  print("- Object with zero value\n");
  print(
    "Length: %.2f, Width: %d, Height: %d, Name: %s, Shape: %s\n\n", 
    obj.Dim.Length, obj.Dim.Width, obj.Dim.Height, obj.Dim.Name, obj.Shape,
  );

  let block : Block = Block{};
  print("- Block with zero value\n");
  print(
    "Top(Length: %.2f, Width: %d, Height: %d, Name: %s, Shape: %s)\n", 
    block.top.Dim.Length, block.top.Dim.Width, block.top.Dim.Height, 
    block.top.Dim.Name, block.top.Shape,
  );
  print(
    "Bottom(Length: %.2f, Width: %d, Height: %d, Name: %s, Shape: %s)\n", 
    block.bottom.Dim.Length, block.bottom.Dim.Width, block.bottom.Dim.Height, 
    block.bottom.Dim.Name, block.bottom.Shape,
  );

  return 0;
}
