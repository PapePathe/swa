structure MultiArray {
  arr1: [2]entier,
  arr2: [3]entier,
  value: entier,
}

demarrer() entier {
  variable m : MultiArray = MultiArray{
    arr1: [2]entier{10, 20},
    arr2: [3]entier{100, 200, 300},
    value: 42,
  };

  afficher("arr1[0]=%d arr1[1]=%d arr2[0]=%d arr2[2]=%d value=%d", 
        m.arr1[0], m.arr1[1], m.arr2[0], m.arr2[2], m.value);
  retourner 0;
}
