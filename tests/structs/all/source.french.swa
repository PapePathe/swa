dialect:french;

demarrer() entier {
  structure Engineer{
    Age: entier,
    Taille: decimal,
    Name: chaine,
    TechStack: chaine,
  }
 
  variable r1: Engineer = Engineer{
    Age: 40,
    Taille: 1.80,
    Name: "Pathe",
    TechStack: "Ruby, Rust, Go"
  };

  afficher(
    "Nom: (%s), Stack Technique: (%s), Age: (%d), Taille: (%.2f)", 
    r1.Name, r1.TechStack, r1.Age, r1.Taille
  );

  retourner 0;
}
