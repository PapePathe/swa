dialect:english;

start() int {
  struct Engineer{
    Age: int,
    Height: float,
    Name: string,
    TechStack: string,
  }

  let r1:Engineer = Engineer{
   Age: 40,
   Height: 1.80,
   Name: "Pathe",
   TechStack: "Ruby, Rust, Go"
  };

  print(
    "Name: (%s), TechStack: (%s), Age: (%d), Height: (%.2f)", 
    r1.Name, r1.TechStack, r1.Age, r1.Height
  );

  return 0;
}
