structure Mixed {
  embedded: [2]entier,
  pointer: *entier,
}

demarrer() entier {
  variable m : Mixed = Mixed{
    embedded: [2]entier{10, 20},
    pointer: [3]entier{100, 200, 300},
  };

  afficher("embedded[0]=%d embedded[1]=%d pointer[0]=%d pointer[2]=%d", 
        m.embedded[0], m.embedded[1], m.pointer[0], m.pointer[2]);
